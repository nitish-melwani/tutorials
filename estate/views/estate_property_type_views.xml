<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="estate_settings_views_action" model="ir.actions.act_window">
        <field name="name">Properties Types</field>
        <field name="res_model">estate_property_type</field>
        <field name="view_mode">tree,form</field>
    </record>


    <record id="estate_property_type_form_tree" model="ir.ui.view">
        <field name="name">estate.property.type.form</field>
        <field name="model">estate_property_type</field>
        <field name="arch" type="xml">
            <form string="Test">
                <sheet>
                <header>
                    <!-- <button type="action" name="estate.property.offer" string="Go to"/> -->
                    <!-- offer_count -->
                </header>
                    <h1><field name="name"/></h1>
                    <notebook>
                        <page string="Description">
                            <field name="property_ids">
                                <tree>
                                    <field name="name"/>
                                    <field name="expected_price"/>
                                    <field name="state"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>


</odoo>